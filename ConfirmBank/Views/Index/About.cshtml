@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>
<form method="post" id="form_Submit">
    <input type="text" name="page_unique" />
    <input type="text" name="page_url" />
    <input type="text" name="page" />
    <input type="submit" value="submit" onclick="send()" />
</form>
<div id="form_View"></div>
<script>

    function send() {
        Data = $("#form_Submit").serialize();
        debugger
        $.ajax({
            traditional: true,
            type: 'Post',
            url: '~/api/ProducList/post',
            cache: false,
            dataType: 'html',
            data: Data,
            success: function (response) {
               
                $("#form_View").html(response);
                

            },
            error: function (ex) {
                alert(ex.responseText);
            }
        });
    }

</script>